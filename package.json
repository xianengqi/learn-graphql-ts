{
  "name": "learn-graphql-ts",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec TS_NODE_FILES=true ts-node src/index.ts",
    "test": "NODE_ENV=test jest --watch",
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
  },
  "devDependencies": {
    "@types/node": "^14.0.26",
    "@types/uuid": "^8.0.0",
    "gql2ts": "^1.10.1",
    "nodemon": "^2.0.4",
    "ts-node": "8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^6.0.15",
    "@graphql-tools/load": "^6.0.15",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.7",
    "apollo-server-express": "^2.16.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql-request": "^2.0.0",
    "graphql-tools": "^6.0.15",
    "graphql-yoga": "^1.18.3",
    "jest": "^26.1.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.1.3",
    "typeorm": "0.2.25"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
